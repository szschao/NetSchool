{
  "name": "mymodule",
  "version": "0.1.0",
  "author": "sunchao",
  "description": "example project template for a cross platform (node and browser) javascript module",
  "scripts": {
    "clean":"grunt cleanup",
    "wbt": "istanbul cover ./node_modules/mocha/bin/_mocha -- ./test/unit-test/**/*.test.js -R spec --colors --bail --require chai --slow 200 --timeout 500 --check-leaks --recursive",
    "bbt": "grunt bbt",
    "sit": "wdio test/wdio.conf.local.js",
    "test": "grunt cleanup && npm run wbt && npm run bbt && npm run sit"
  },
  "main": "mymodule",
  "dependencies": {
    "minimatch": "~3.0.4"
  },
  "devDependencies": {t
    "browserify": "~14.5.0",
    "chai": "~4.1.2",
    "grunt": "~1.0.1",
    "grunt-browserify": "~5.2.0",
    "grunt-contrib-clean": "~1.1.0",
    "grunt-contrib-connect": "~1.0.2",
    "grunt-contrib-jshint": "~1.1.0",
    "grunt-contrib-uglify": "~3.1.0",
    "grunt-contrib-watch": "~1.0.0",
    "grunt-mocha-phantomjs": "~4.0.0",
    "grunt-mocha-test": "~0.13.3",
    "istanbul": "~0.4.5",
    "sinon": "~4.1.1",
    "sinon-chai": "~2.14.0",
    "request": "~2.83.0",
    "express": "^4.16.2",
    "mocha": "~4.0.1",
    "wdio-mocha-framework": "^0.5.11",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-spec-reporter": "^0.1.2",
    "wdio-testingbot-service": "^0.1.7",
    "webdriverio": "^4.9.9"
  },
  "browser": {
    "./lib/echo.js": "./browser/lib/shim/echo.js",
    "minimatch": "./browser/lib/shim/minimatch.js",
    "util": "./browser/lib/shim/node-util.js",
    "./lib/optional": false
  },
  "testling": {
    "browsers": [
      "ie9",
      "ie10",
      "ff/15",
      "ff/20",
      "ff/23",
      "ff/nightly",
      "chrome/29",
      "chrome/canary",
      "opera/15",
      "opera/next",
      "safari/6.0"
    ],
    "harness": "mocha",
    "files": "test/unit-test/*.js"
  },
  "engines": {
    "node": ">= 0.10.x"
  }
}
